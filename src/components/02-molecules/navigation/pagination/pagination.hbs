<nav>
  <h3 class="sr-only">Pagination</h2>
  <ul class="list-inline text-center">
    {{#if previous}}<li class="list-inline-item"><a href="{{ previous.url }}" class="btn btn-secondary vd-btn-chevron-l">{{ previous.text }}</a></li>{{/if}}
    {{#if next}}<li class="list-inline-item"><a href="{{ next.url }}" class="btn btn-secondary vd-btn-chevron-r">{{ next.text }}</a></li>{{/if}}
  </ul>
</nav>
